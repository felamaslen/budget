{
  "name": "budget",
  "version": "3.3.5",
  "description": "Personal finance app",
  "main": "index.js",
  "engines": {
    "node": "8.9.x"
  },
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "postinstall": "NODE_ENV=production ./node_modules/.bin/webpack",
    "build": "NODE_ENV=production DOTENV_INJECT=true ./node_modules/.bin/webpack",
    "start": "node ./api/src/index.js",
    "dev": "NODE_ENV=development node index.js",
    "dev:api": "NODE_ENV=development SKIP_APP=true nodemon --inspect -w ./api/src index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register -R spec \"./web/test/**/*.spec.js\" \"./api/test/**/*.spec.js\"",
    "lint": "./node_modules/.bin/eslint webpack.config.js webpack api/src api/test web/src web/test",
    "sass-lint": "./node_modules/.bin/sass-lint -qv \"./web/src/sass/**/*.scss\"",
    "scrape_funds": "NODE_ENV=production node ./api/scripts/scrapeFunds.js -q",
    "dev:scrape_funds": "NODE_ENV=development node ./api/scripts/scrapeFunds.js -v",
    "test:scrape_funds": "NODE_ENV=testintegration node ./api/scripts/scrapeFunds.js -v",
    "scrape_holdings": "NODE_ENV=production node ./api/scripts/scrapeFunds.js -h",
    "test:scrape_holdings": "NODE_ENV=testintegration node ./api/scripts/scrapeFunds.js -h -v",
    "dev:scrape_holdings": "NODE_ENV=development node ./api/scripts/scrapeFunds.js -h -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/budget/issues"
  },
  "homepage": "https://github.com/felamaslen/budget#readme",
  "babel": {
    "env": {
      "include": [
        "transform-class-properties"
      ],
      "production": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "ie >= 10"
                ]
              }
            }
          ],
          "stage-0"
        ]
      },
      "development": {
        "presets": [
          "react",
          "stage-0"
        ],
        "sourceMaps": "inline"
      },
      "test": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "node": "8"
              }
            }
          ],
          "stage-0"
        ]
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "dateformat": "^2.0.0",
    "deepmerge": "^2.0.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "express-cache-control": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "import-glob-loader": "^1.1.0",
    "joi": "^13.1.2",
    "joi-date-extensions": "^1.1.1",
    "jsonp": "^0.2.1",
    "knex": "^0.14.4",
    "md5": "^2.2.1",
    "moment": "^2.21.0",
    "morgan": "^1.8.2",
    "mysql2": "^1.5.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "prompt": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.1.1",
    "react-media": "^1.6.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sass-loader": "^6.0.6",
    "sha1": "^1.1.1",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-dist": "^3.1.7",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "deep-equal": "^1.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "fs-extra": "^4.0.1",
    "it-each": "^0.3.1",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^3.3.1",
    "redux-test-utils": "^0.2.2",
    "sass-lint": "^1.11.1",
    "style-loader": "^0.19.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}

{
  "name": "budget",
  "version": "4.10.2",
  "description": "Personal finance app",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "npm run check-types -- --watch",
    "build:server": "npm run check-types && rm -rf dist && BABEL_ENV=node rollup -c",
    "build:client": "rm -rf public && NODE_ENV=production webpack --mode production",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist",
    "dev:run": "NODE_ENV=development BABEL_ENV=node babel-watch -e .ts,.tsx,.js -L -w src/server -D src/server/index.ts",
    "dev": "docker-compose up --build",
    "dev:api": "SKIP_APP=true npm run dev",
    "test": "TZ=UTC jest",
    "lint": "eslint webpack.config.js src",
    "scrape_funds": "node ./dist/scripts/scrape-funds --prices",
    "dev:scrape_funds": "NODE_ENV=development BABEL_ENV=node babel-node ./src/scripts/scrape-funds --prices",
    "test:scrape_funds": "NODE_ENV=testintegration BABEL_ENV=node babel-node ./src/scripts/scrape-funds --prices",
    "scrape_holdings": "node ./dist/scripts/scrape-funds --holdings",
    "dev:scrape_holdings": "NODE_ENV=development babel-node ./src/scripts/scrape-funds --holdings",
    "test:scrape_holdings": "NODE_ENV=testintegration babel-node ./src/scripts/scrape-funds --holdings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/budget/issues"
  },
  "homepage": "https://github.com/felamaslen/budget#readme",
  "_moduleAliases": {
    "~": "./src"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "fs-extra": "^8.1.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-url": "^4.0.0",
    "@svgr/rollup": "^4.3.3",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23",
    "@types/koa": "^2.11.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/parser": "^2.9.0",
    "autoprefixer": "^9.7.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "jest": "^24.9.0",
    "koa-helmet": "^5.2.0",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^8.5.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}

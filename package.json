{
  "name": "budget",
  "sideEffects": [
    "*.scss"
  ],
  "version": "4.13.1",
  "description": "Personal finance app",
  "main": "api/build/index.js",
  "engines": {
    "node": "10"
  },
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "yarn check-types --watch",
    "build:api": "NODE_ENV=production BABEL_ENV=node babel api/src -d api/build --extensions \".ts,.js\" --copy-files",
    "build:web": "NODE_ENV=production webpack --mode production",
    "build": "yarn check-types && yarn build:api && yarn build:web",
    "start": "node api/build",
    "dev": "docker-compose up --build",
    "dev:api": "SKIP_APP=true yarn dev",
    "test:api": "BABEL_ENV=node NODE_ENV=test TZ=UTC jest -c jest.api.config.js --detectOpenHandles --runInBand --forceExit",
    "test:client": "BABEL_ENV=node NODE_ENV=test TZ=UTC jest -c jest.client.config.js --detectOpenHandles --runInBand --forceExit",
    "test": "yarn test:api && yarn test:client",
    "lint": "eslint webpack.config.js api/src api/test web/src web/test",
    "sass-lint": "sass-lint -qv \"./web/src/**/*.scss\"",
    "knex:migrate:make": "knex --knexfile knexfile.ts -x js migrate:make",
    "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:seed:run": "knex --knexfile knexfile.ts seed:run",
    "scrape": "node ./api/build/scripts/scrape-funds",
    "scrape_funds": "yarn scrape -- --prices",
    "scrape_holdings": "yarn scrape -- --holdings",
    "dev:scrape": "docker-compose run budget ./node_modules/.bin/ts-node -r tsconfig-paths/register ./api/src/scripts/scrape-funds",
    "dev:scrape_funds": "yarn dev:scrape -- --prices",
    "dev:scrape_holdings": "yarn dev:scrape -- --holdings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",

{
  "name": "budget",
  "version": "3.0.1-next",
  "description": "Personal finance app",
  "main": "api/src/index.js",
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "preinstall": "rm -rf ./node_modules",
    "postinstall": "./node_modules/.bin/webpack",
    "start": "NODE_ENV=production node ./api/src/index.js",
    "dev:api": "NODE_ENV=development nodemon -w ./api/src ./api/src/index.js",
    "dev:wds": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server",
    "test": "./node_modules/.bin/mocha -w \"./api/test/**/*.spec.js\" --bail --reporter spec",
    "test:web": "./node_modules/.bin/mocha --compilers js:babel-core/register -w \"./web/test/**/*.spec.js\" --bail --reporter spec",
    "test:webui": "./node_modules/.bin/mocha -w \"./web/test.ui/index.js\" --bail --reporter spec",
    "lint": "./node_modules/.bin/eslint webpack.config.js webpack api/src api/test web/src web/test",
    "sass-lint": "./node_modules/.bin/sass-lint -qv \"./web/src/sass/**/*.scss\"",
    "install_db": "node ./api/scripts/createSchema.js",
    "test:install_db": "NODE_ENV=test node ./api/scripts/createSchema.js",
    "scrape_funds": "node ./api/scripts/scrapeFunds.js -q",
    "dev:scrape_funds": "NODE_ENV=development node ./api/scripts/scrapeFunds.js -v",
    "test:scrape_funds": "NODE_ENV=test node ./api/scripts/scrapeFunds.js -v",
    "scrape_holdings": "node ./api/scripts/scrapeFunds.js -h",
    "test:scrape_holdings": "NODE_ENV=test node ./api/scripts/scrapeFunds.js -h -v",
    "dev:scrape_holdings": "NODE_ENV=development node ./api/scripts/scrapeFunds.js -h -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/budget/issues"
  },
  "homepage": "https://github.com/felamaslen/budget#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "css-loader": "^0.28.5",
    "dateformat": "^2.0.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "md5": "^2.2.1",
    "mysql": "^2.14.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "prompt": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-media": "^1.6.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sha1": "^1.1.1",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-dist": "^3.1.7",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "fs-extra": "^4.0.1",
    "it-each": "^0.3.1",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "react-test-renderer": "^15.6.1",
    "sass-lint": "^1.11.1",
    "webpack-dev-server": "^2.7.1"
  }
}

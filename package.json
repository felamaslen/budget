{
  "name": "budget",
  "version": "4.2.8",
  "description": "Personal finance app",
  "main": "index.js",
  "engines": {
    "node": "8.9.x"
  },
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "postinstall": "NODE_ENV=production webpack --mode production",
    "start": "node index.js",
    "dev": "NODE_ENV=development node index.js",
    "dev:api": "NODE_ENV=development SKIP_APP=true nodemon --inspect -w ./api/src index.js",
    "test": "NODE_ICU_DATA=node_modules/full-icu NODE_ENV=test mocha --require babel-core/register --require ignore-styles -R spec \"./web/test/**/*.spec.js\" \"./api/test/**/*.spec.js\"",
    "lint": "eslint webpack.config.js webpack api/src api/test web/src web/test",
    "sass-lint": "sass-lint -qv \"./web/src/sass/**/*.scss\"",
    "scrape_funds": "node ./api/scripts/scrapeFunds/index.js --prices",
    "dev:scrape_funds": "NODE_ENV=development node ./api/scripts/scrapeFunds/index.js --prices",
    "test:scrape_funds": "NODE_ENV=testintegration node ./api/scripts/scrapeFunds/index.js --prices",
    "scrape_holdings": "node ./api/scripts/scrapeFunds/index.js --holdings",
    "dev:scrape_holdings": "NODE_ENV=development node ./api/scripts/scrapeFunds/index.js --holdings",
    "test:scrape_holdings": "NODE_ENV=testintegration node ./api/scripts/scrapeFunds/index.js --holdings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/budget/issues"
  },
  "homepage": "https://github.com/felamaslen/budget#readme",
  "babel": {
    "env": {
      "include": [
        "transform-class-properties"
      ],
      "production": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "ie >= 10"
                ]
              }
            }
          ],
          "stage-0"
        ]
      },
      "development": {
        "presets": [
          "react",
          "stage-0"
        ],
        "sourceMaps": "inline"
      },
      "test": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "node": "8"
              }
            }
          ],
          "stage-0"
        ]
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "command-line-args": "^5.0.2",
    "cssnano": "^3.10.0",
    "dateformat": "^2.0.0",
    "deepmerge": "^2.0.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "express-cache-control": "^1.0.2",
    "history": "^4.7.2",
    "humanize-duration": "^3.14.0",
    "immutable": "^3.8.1",
    "import-glob-loader": "^1.1.0",
    "intl-locales-supported": "^1.0.0",
    "joi": "^13.1.2",
    "joi-date-extensions": "^1.1.1",
    "jsonp": "^0.2.1",
    "jwt-simple": "^0.5.1",
    "knex": "^0.14.4",
    "luxon": "^0.5.3",
    "md5": "^2.2.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.5.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "prompt": "^1.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-media": "^1.6.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sha1": "^1.1.1",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-dist": "^3.1.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@epegzz/sass-vars-loader": "^3.3.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "css-loader": "^0.28.11",
    "deep-equal": "^1.0.1",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^1.1.11",
    "fixture-factory": "^1.5.1",
    "fs-extra": "^4.0.1",
    "full-icu": "^1.2.1",
    "ignore-styles": "^5.0.1",
    "it-each": "^0.3.1",
    "jsdom": "^11.2.0",
    "match-media-mock": "^0.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^3.5.0",
    "mock-knex": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^3.3.1",
    "redux-test-utils": "^0.2.2",
    "sass-lint": "^1.11.1",
    "sass-loader": "^7.0.1",
    "sass-values-loader": "^1.0.4",
    "sass-variable-loader": "^0.1.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  }
}

{
  "name": "budget",
  "version": "4.10.2",
  "description": "Personal finance app",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "npm run check-types -- --watch",
    "build:server": "npm run check-types && rm -rf dist && BABEL_ENV=node rollup -c",
    "build:client": "rm -rf public && NODE_ENV=production webpack --mode production",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist",
    "dev:run": "NODE_ENV=development BABEL_ENV=node babel-watch -e .ts,.tsx,.js -L -w src/server -D src/server/index.ts | roarr pretty-print",
    "dev": "docker-compose up --build",
    "dev:api": "SKIP_APP=true npm run dev",
    "test": "TZ=UTC BABEL_ENV=node jest",
    "test:dev": "npm run test -- --watch",
    "lint": "BABEL_ENV=node eslint webpack.config.js \"src/**/*.ts*\"",
    "scrape_funds": "node ./dist/scripts/scrape-funds --prices",
    "dev:scrape_funds": "NODE_ENV=development BABEL_ENV=node babel-node ./src/scripts/scrape-funds --prices",
    "test:scrape_funds": "NODE_ENV=testintegration BABEL_ENV=node babel-node ./src/scripts/scrape-funds --prices",
    "scrape_holdings": "node ./dist/scripts/scrape-funds --holdings",
    "dev:scrape_holdings": "NODE_ENV=development babel-node ./src/scripts/scrape-funds --holdings",
    "test:scrape_holdings": "NODE_ENV=testintegration babel-node ./src/scripts/scrape-funds --holdings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/budget.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/budget/issues"
  },
  "homepage": "https://github.com/felamaslen/budget#readme",
  "_moduleAliases": {
    "~": "./src"
  },
  "lint-staged": {
    "**/.ts?(x)": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@redux-saga/core": "^1.1.3",

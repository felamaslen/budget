{
  "name": "budget",
  "version": "5.0.2",
  "description": "Personal finance app",
  "main": "api/build/index.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "yarn check-types --watch",
    "check-types:watch:api": "tsc -p api/tsconfig.json --watch",
    "check-types:watch:client": "tsc -p web/tsconfig.json --watch",
    "gen-gql-types": "graphql-codegen --config codegen.yml",
    "minify-introspection": "python -c 'import json,sys; json.dump(json.load(sys.stdin), fp=sys.stdout, separators=(\",\",\":\"))' < api/src/introspection.json > api/build/introspection.json",
    "build:api": "rm -rf api/build && BABEL_ENV=node babel api/src -d api/build --extensions \".ts,.js\" --copy-files",
    "build:client": "NODE_ENV=production webpack --mode production",
    "build": "yarn check-types && yarn build:api && yarn minify-introspection && yarn build:client",
    "start": "node api/build",
    "dev": "docker-compose up",
    "dev:api": "SKIP_APP=true yarn dev",
    "test:api:unit": "TZ=UTC jest -w 4 -c jest.api.unit.config.js --detectOpenHandles --forceExit",
    "test:api:integration": "TZ=UTC jest -w 1 -c jest.api.integration.config.js --detectOpenHandles --forceExit",
    "test:client": "TZ=UTC jest -w 4 -c jest.client.config.js --detectOpenHandles --forceExit",
    "test": "yarn test:api:unit && yarn test:api:integration && yarn test:client",
    "coverage": "yarn ts-node-script ./scripts/merge-coverage.ts --report ./coverage/api/unit/coverage-final.json --report ./coverage/api/integration/coverage-final.json --report ./coverage/web/coverage-final.json",
    "lint": "eslint webpack.config.js \"api/src/**/*.ts\" \"web/src/**/*.ts\" \"web/src/**/*.tsx\"",
    "ts-prune": "sh ./scripts/ts-prune.sh",
    "knex": "sh ./scripts/knex.sh",
    "scrape": "node ./api/build/scripts/scrape-funds",
    "scrape_funds": "yarn scrape --prices",
    "scrape_holdings": "yarn scrape --holdings",
    "dev:scrape": "docker-compose run budget ./node_modules/.bin/ts-node -r tsconfig-paths/register ./api/src/scripts/scrape-funds",
    "dev:scrape_funds": "yarn dev:scrape --prices",
    "dev:scrape_holdings": "yarn dev:scrape --holdings"
  },

    "fast-memoize": "^2.5.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^4.0.1",
    "ignore-styles": "^5.0.1",
    "it-each": "^0.3.1",
    "jsdom": "^11.2.0",
    "just-compose": "^1.1.0",
    "match-media-mock": "^0.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "ninos": "^2.0.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
